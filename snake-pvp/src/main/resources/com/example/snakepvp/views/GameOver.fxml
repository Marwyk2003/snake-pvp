<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.net.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane id="AnchorPane" prefHeight="600" prefWidth="740"
            xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.snakepvp.views.GameOver">
    <children>
        <ImageView fx:id="spotlight" AnchorPane.rightAnchor="30.0" AnchorPane.BottomAnchor="50.0" />
        <ImageView fx:id="cup" AnchorPane.rightAnchor="100.0" AnchorPane.BottomAnchor="40.0" />
        <Button fx:id="returnButton" mnemonicParsing="false" AnchorPane.leftAnchor="50.0" AnchorPane.TopAnchor="50.0" onAction="#returnButtonAction" onMouseEntered="#mouseAction" />
        <Label fx:id="gameOverLabel" AnchorPane.leftAnchor="200.0" AnchorPane.TopAnchor="100.0" text="GAME OVER" styleClass="label" />
    </children>
    <stylesheets>
        <URL value="@../../../../styles.css" />
    </stylesheets>
</AnchorPane>