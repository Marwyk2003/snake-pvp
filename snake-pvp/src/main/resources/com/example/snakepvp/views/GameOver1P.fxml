<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.net.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.shape.Circle?>

<AnchorPane id="AnchorPane" prefHeight="700" prefWidth="1000"
            xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.snakepvp.views.GameOver1P">
    <children>
        <Circle fx:id="spotlight" AnchorPane.rightAnchor="50.0" AnchorPane.BottomAnchor="80.0" />
        <ImageView fx:id="trash" AnchorPane.rightAnchor="120.0" AnchorPane.BottomAnchor="60.0" />
        <Button fx:id="returnButton" mnemonicParsing="false" AnchorPane.leftAnchor="50.0" AnchorPane.TopAnchor="50.0" onAction="#returnButtonAction" onMouseEntered="#mouseAction" />
        <Label fx:id="gameLostLabel1" AnchorPane.leftAnchor="160.0" AnchorPane.TopAnchor="100.0" text="GAME OVER" />
        <Label fx:id="gameLostLabel2" AnchorPane.leftAnchor="160.0" AnchorPane.TopAnchor="150.0" text="score: 34" />
    </children>
    <stylesheets>
        <URL value="@../../../../styles.css" />
    </stylesheets>
</AnchorPane>